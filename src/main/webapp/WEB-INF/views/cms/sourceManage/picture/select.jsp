<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %><div id="imggrid"></div><script>    $('#imggrid').imgGrid({//        title: '图片集',        type:'post',//GET|POST        url: contextPath + '/sourceManage/picture/list',//url        params: { type: 0 },//url参数        height: '500px',        width: '870px',        img: { /*height: '170px', */animate: 'zoomIn' },//图片宽度，及动画效果        onClick: function (obj,index, item) {//点击图片事件            alert(item.id);        },        render: function (item, index) {//自定义显示图片            var str = '<img style="height:170px;" src="' + item.url + '?imageMogr2/thumbnail/x170">';            return str;        }    });</script>