<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %><input type="hidden" name="id" value="${auxiliarySectionTemplate.id}"/><input type="hidden" name="columnId" value="${columnId}"/><input type="hidden" id="sectionTemplateId" name="sectionTemplateId" value="${auxiliarySectionTemplate.sectionTemplate.id}" /><h4 class="widget-title lighter smaller">专题模版附属信息</h4><div class="col-lg-6">	<div class="form-group">		<label class="col-lg-3 control-label">专题模版:</label><span class="text-danger">*</span>		<div class="col-lg-6">			<input type="text" class="form-control" id="sectionTemplateName" name="" value="${auxiliaryFallTemplate.sectionTemplate.name}" readonly="readonly"/>		</div>		<div class="col-lg-2">			<input id="Select_btnSearch" name="Select_btnSearch" type="button" value="选择"  onclick="selectSectionTemplate()" class="btn btn-primary btn-sm form-control">			</input>		</div>		</div>    </div><script type="text/javascript">	/**	 * 瀑布流模版选择	 */	function selectSectionTemplate(){		showSelectDlg("选择瀑布流模版", contextPath + '/template/sectionTemplate/select', ["Select_table-data"], 				selectSectionTemplateCallBack,BootstrapDialog.SIZE_WIDE);	}	function selectSectionTemplateCallBack(obj){	    $("#sectionTemplateName").val(obj[0].name);	    $("#sectionTemplateId").val(obj[0].id);	}    jQuery(function ($) {        $('#formDlgOther').validate(            {                errorElement: 'span',                errorClass: 'help-block',                focusCleanup: false,                focusInvalid: false,                onsubmit: false,                rules: {                    "sectionTemplateName": {required: true}                },                messages: {                    "sectionTemplateName": '请选择专题模版'                },                highlight: function (e) {                    $(e).closest('.form-group').removeClass('has-info')                        .addClass('has-error');                },                success: function (e) {                    $(e).closest('.form-group').removeClass('has-error')                        .addClass('has-success');                    $(e).remove();                },            });    });</script>